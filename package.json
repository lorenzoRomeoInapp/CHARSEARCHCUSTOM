{
  "name": "charsearchcustom",
  "version": "1.0.0",
  "description": "App ricerca caratteristiche lineari IAM",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/core": "^1.54.2",
    "@sap-cloud-sdk/http-client": "^3.5.0",
    "@sap/audit-logging": "^5.7.1",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xsenv": "^4.0.0",
    "@sap/xssec": "^3.3.4",
    "axios": "^1.5.0",
    "express": "^4",
    "passport": "^0.6.0",
    "sqlite3": "^5"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/ux-specification": "^1.108.14",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-charsearchcustom": "cds watch --open charsearchcustom/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy charsearchcustom --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-test": "cds watch --open test/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "auth": "xsuaa",
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      },
      "ZIAMGW_FIORI_TOOL_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZIAMGW_FIORI_TOOL_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "[production]": {
          "credentials": {
            "destination": "IAMGTW",
            "path": "/sap/opu/odata/sap/ZIAMGW_FIORI_TOOL_SRV"
          }
        }
      }
    },
    "features": {
      "in_memory_db": true
    }
  },
  "sapux": [
    "app/charsearchcustom"
  ]
}